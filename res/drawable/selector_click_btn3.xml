<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/transparent_black2"></solid>
            <corners android:radius="3dp"></corners>
        </shape>
    </item>
    <item>
        <!--shape_noticebg-->
        <shape>
            <solid android:color="@color/transparent">
            </solid>
            <corners android:radius="3dp"></corners>
            <stroke android:width="1dp" android:color="@color/shape_noticebg"></stroke>
        </shape>

    </item>
</selector>